    "precommit": "yarn check:all && yarn fix:all $1",
    "cache:cleaner": "node scripts/dev-workspace-cleaner.js $*",
    "rename": "rm -rf ios android .git && npx react-native eject && yarn cache:cleaner && git init && git add . && git commit -m 'Initial commit'",
